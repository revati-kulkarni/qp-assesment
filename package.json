{
    "name": "grocery-booking-api",
    "version": "1.0.0",
    "description": "A Grocery Booking API built with Node.js, Express, TypeScript, and PostgreSQL.",
    "main": "dist/server.js",
    "scripts": {
      "start": "tsc && node  ./dist/server.js",
      "build": "tsc",
      "lint": "eslint . --ext .ts",
      "migrate": "sequelize-cli db:migrate",
      "seed": "sequelize-cli db:seed:all"
    },
    "keywords": [
      "grocery",
      "booking",
      "api",
      "express",
      "typescript",
      "sequelize",
      "postgresql"
    ],
    "author": "Revati",
    "license": "MIT",
    "dependencies": {
      "bcrypt": "^5.1.0",
      "dotenv": "^16.0.3",
      "express": "^4.18.2",
      "jsonwebtoken": "^9.0.0",
      "pg": "^8.11.0",
      "pg-hstore": "^2.3.4",
      "sequelize": "^6.32.1"
    },
    "devDependencies": {
      "@types/bcrypt": "^5.0.0",
      "@types/express": "^4.17.17",
      "@types/jsonwebtoken": "^9.0.2",
      "@types/node": "^20.4.2",
      "@typescript-eslint/eslint-plugin": "^5.62.0",
      "@typescript-eslint/parser": "^5.62.0",
      "eslint": "^8.45.0",
      "nodemon": "^3.0.1",
      "sequelize-cli": "^6.6.1",
      "typescript": "^5.2.2"
    }
  }
  